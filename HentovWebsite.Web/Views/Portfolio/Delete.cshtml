@using HentovWebsite.Utility
@model HentovWebsite.Models.View.Portfolio.ProjectViewModel

@{
    ViewBag.Title = "Delete Project - Hentov.NET";
}

<div class="container">
    @using (Html.BeginForm())
    {
        <h4>@WebText.BlogDeleteHead</h4>
        <div class="form-group">
            <div class="col-md-10">
                @Html.ActionLink(@WebText.BlogDeleteNo, "Index");
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-10">
                @Html.HiddenFor(model => model.Id);
                <input type="submit" value=@WebText.BlogDeleteYes class="btn btn-default" />
            </div>
        </div>
    }
</div>